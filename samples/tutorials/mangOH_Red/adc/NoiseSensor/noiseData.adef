sandboxed: true
start: manual

executables:
{
    noiseData = ( noiseDataComponent )
}

processes:
{
    envVars:
    {
        LE_LOG_LEVEL = DEBUG
    }
    run:
    {
        ( noiseData )
    }
    faultAction: restart
}

bindings:
{
    noiseData.noiseDataComponent.le_adc -> modemService.le_adc
}
