sandboxed: false

executables:
{
    NtagDhubIf = (NtagDhubIfComponent)
}

bindings:
{
    NtagDhubIf.NtagDhubIfComponent.fieldDetectGpio -> gpioService.le_gpioPin23
}

extern:
{
    NtagDhubIf.NtagDhubIfComponent.dhubIO  // required API
}

processes:
{

    run:
    {
        ( NtagDhubIf )
    }

    envVars:
    {
        LE_LOG_LEVEL = DEBUG
    }

    faultAction: stopApp
}

